{% extends 'russian/russian.base.html.twig' %}

{% block main_content %}
	<div id="content" class="slike">
		<ul>
			{% for filename in filenames %}
				<li>
					<div class="thumb fancybox">
						<a href="/gfx/photo/large/{{ filename }}" target="_blank" rel="gallery">
							<img src="/gfx/photo/mini/{{ filename }}" alt=""/>
						</a>
					</div>
				</li>
			{% endfor %}
		</ul>
		<div style="clear:both"></div>
		<a class="toVrh soft" href="#vrh" title="В начало страницы">&nbsp;</a>
	</div><!--#content-->
{% endblock %}
